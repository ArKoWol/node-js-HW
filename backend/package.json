{
  "name": "article-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend server for managing articles",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:status": "npx sequelize-cli db:migrate:status",
    "db:migrate-data": "node migrate-data-from-files.js",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "assign-admin": "node scripts/assign-admin.js",
    "check-creator-ids": "node scripts/check-creator-ids.js",
    "backfill-creator-ids": "node scripts/backfill-creator-ids.js"
  },
  "keywords": [
    "articles",
    "backend",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "ws": "^8.18.3"
  }
}
